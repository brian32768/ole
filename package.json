{
   "name": "@map46/ole",
   "version": "1.0.1",
   "description": "Library to integrate OpenLayers 5 with Esri ArcGIS Server REST API. FORK of boundlessgeo/ole.",
   "homepage": "https://github.com/brian32768/ole#readme",
   "keywords": [
      "mapping",
      "openlayers", "ol",
      "esri", "arcgis",
      "rest"
   ],
   "main": "index.html",
   "scripts": {
      "start": "parcel service index.html --open",
      "test": "parcel service packagetest.html --open",
      "build": "npm run clean && npm run transpile && node tasks/prepare-package && cp README.md build/@map46/ole",
      "clean": "rm -rf build/*",
      "transpile": "babel src --out-dir build --source-maps"
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/brian32768/ole.git"
   },
   "author": "Boundless",
   "contributors": "Brian H Wilson",
   "license": "Apache-2.0",
   "bugs": {
      "url": "https://github.com/brian32768/ole/issues",
      "email": "brian+ole@wildsong.biz"
   },
   "devDependencies": {
      "@babel/cli": "^7.2.0",
      "@babel/core": "^7.2.2",
      "@babel/preset-env": "^7.2.3",
      "lint": "^1.1.2",
      "parcel-bundler": "^1.11.0"
   },
   "dependencies": {
      "jquery": "^3.3.1",
      "ol": "^5.3.0"
   }
}
